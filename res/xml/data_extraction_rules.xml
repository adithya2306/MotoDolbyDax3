<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfNoEncryptionCapabilities="true">
        <exclude domain="file" path="string" />
        <exclude domain="database" path="string" />
        <exclude domain="sharedpref" path="string" />
        <exclude domain="external" path="string" />
        <exclude domain="root" path="string" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="file" path="string" />
        <exclude domain="database" path="string" />
        <exclude domain="sharedpref" path="string" />
        <exclude domain="external" path="string" />
        <exclude domain="root" path="string" />
    </device-transfer>
</data-extraction-rules>